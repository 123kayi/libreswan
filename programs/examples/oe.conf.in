
			
conn packetdefault
		type=tunnel
		left=%defaultroute
		leftsubnet=0.0.0.0/0
		leftnexthop=%defaultroute
		right=%opportunistic
		failureshunt=passthrough
		keyingtries=3
		leftid=%myid
		leftrsasigkey=%dnsondemand
		rightrsasigkey=%dnsondemand
		ikelifetime=1h
		keylife=1h
		rekey=no
		auto=route

conn clear
		type=passthrough
		left=%defaultroute
		leftid=%myid
		leftrsasigkey=%dnsondemand
		rightrsasigkey=%dnsondemand
		authby=never
		right=%group
		leftnexthop=%defaultroute
		auto=route

conn clear-or-private
		type=passthrough
		left=%defaultroute
		leftid=%myid
		leftrsasigkey=%dnsondemand
		rightrsasigkey=%dnsondemand
		right=%opportunisticgroup
		leftnexthop=%defaultroute
		failureshunt=passthrough
		keyingtries=3
		ikelifetime=1h
		keylife=1h
		rekey=no
		auto=route

conn private-or-clear
		type=tunnel
		left=%defaultroute
		leftid=%myid
		leftrsasigkey=%dnsondemand
		rightrsasigkey=%dnsondemand
		right=%opportunisticgroup
		leftnexthop=%defaultroute
		failureshunt=passthrough
		keyingtries=3
		ikelifetime=1h
		keylife=1h
		rekey=no
		auto=route
		

conn private
		type=tunnel
		left=%defaultroute
		right=%opportunisticgroup
		leftid=%myid
		leftrsasigkey=%dnsondemand
		rightrsasigkey=%dnsondemand
		leftnexthop=%defaultroute
		failureshunt=drop
		keyingtries=3
		ikelifetime=1h
		keylife=1h
		rekey=no
		auto=route
		

conn block
		type=reject
		left=%defaultroute
		authby=never
		leftid=%myid
		leftrsasigkey=%dnsondemand
		rightrsasigkey=%dnsondemand
		right=%group
		leftnexthop=%defaultroute
		auto=route
	


