<network>
  <name>192_1_3_0</name>
  <bridge name='swan13' stp='on' delay='0' />
  <mac address='12:00:00:32:64:BA'/>
  <ip address='192.1.3.254' netmask='255.255.255.0'>
    <dhcp>
      <range start='192.1.3.128' end='192.1.3.196' />
    </dhcp>
  </ip>
</network>
